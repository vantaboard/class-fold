{
	"name": "class-fold",
	"displayName": "Class Fold",
	"description": "Folding utility for class names.",
	"version": "0.0.1",
	"repository": "https://github.com/vantaboard/class-fold",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:javascriptreact",
		"onLanguage:typescriptreact",
		"onLanguage:html"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "class-fold.printSymbols",
				"title": "Print Symbols"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.26.0",
		"typescript": "^5.1.3"
	}
}
